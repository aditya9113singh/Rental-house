<%layout("/layouts/boilerplate")%>
<body>

    <!--<form method="GET" action="/listings/new">
        <button>create new </button>
    </form>-->
    <pre>  <h1> All Lists</h3></pre>
    <div class="row row-cols-lg-4 row-cols-md-2 row-cols-sm-1">
      
      <% for(let list of allLists){ %><a href="/listings/<%=list._id%>" class="listing-link">
    <div class="card col listing-card" style="width: 20rem;">
        <img src="<%=list.image%>" class="card-img-top" style="height: 21rem;"alt="listing_image">
        <div class="card-body">
          <p class="card-text">
            <b><%=list.title%></b><br>
            &#8377;<%=list.price%>/night
          </p>
        </div>
      </div>
    </a>
      <% } %>
    </div>
</body>